<html>
    <head>
        <meta http-equiv="content-type" content="text/html; charset=utf-8" />
        <title>`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`</title>
        
        <script type="text/javascript" charset="utf-8">
           
           
            function average_grade() { var args = Array.prototype.slice.call(arguments);
  var sum = 0;
    for (let i=0; i<args.length; i++) {
        sum += Number(args[i]);
    }
  var average = sum / args.length;
  if (average < 35) {
      return "<35";
    }
    if (average >= 35 && average < 45) {
      return "35-45";
    }
    if (average >= 45 && average < 60) {
      return "45-60";
    }
    if (average >= 60 && average < 75) {
      return "60-75";
    }
    return "A";
    }

//имаме нещо като масив(но не е), на който му взимаме средноаритметично от парамрите му
console.log(average_grade(35));//35/1=35
console.log(average_grade(34,36));//70/2=35
console.log(average_grade('34','36'));
console.log(average_grade("34","36"));
        </script>
        
        
        
    </head>
<body>


